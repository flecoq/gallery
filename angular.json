{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "babylon-wp": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "viewEncapsulation": "None"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/dev",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/styles/main.scss",
              "src/assets/bootstrap/lib/bootstrap/css/bootstrap.min.css",
              "src/assets/bootstrap/lib/font-awesome/css/font-awesome.css",
              "src/assets/bootstrap/css/style.css",
              "src/assets/bootstrap/css/modal.css",
              "src/assets/bootstrap/css/style-responsive.css",
              "src/assets/bootstrap/lib/dropzone/css/dropzone.css/",
              "src/assets/responsive/css/responsive.css",
              "src/assets/responsive/css/treeview.css",
              "src/assets/responsive/css/visit.css"
            ],
            "scripts": [
			  "node_modules/jquery/dist/jquery.min.js",
              "src/assets/bootstrap/lib/jquery/jquery.min.js",
              "src/assets/bootstrap/lib/bootstrap/js/bootstrap.min.js",
              "src/assets/bootstrap/lib/jquery.dcjqaccordion.2.7.js",
              "src/assets/bootstrap/lib/jquery.scrollTo.min.js",
              "src/assets/bootstrap/lib/jquery.nicescroll.js",
              "src/assets/bootstrap/lib/common-scripts.js",
              "src/assets/bootstrap/lib/dropzone/dropzone.js",
              "src/assets/bootstrap/lib/modal.js",
			  "src/js/earcut.min.js"
			],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            }
          },
          "configurations": {
            "production": {
              "outputPath": "dist/prod",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "babylon-wp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "babylon-wp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "babylon-wp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "./src/karma.conf.js",
            "scripts": [],
            "styles": [
              "src/styles/main.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "babylon-wp-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "babylon-wp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-dummy:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "babylon-wp",
  "cli": {
    "analytics": "39c771ba-4917-4dc2-96ac-9bae9bff4073"
  }
}
